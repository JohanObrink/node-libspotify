  "immed": true,
  "indent": 2,
  "latedef": true,
  "maxparams": 5,
  "maxdepth": 3,
  "maxcomplexity": 4,
  "newcap": true,
  "noarg": true,
  "sub": true,
  "undef": true,
  "unused": true,
  "quotmark": "single",
  "node": true,
  "globals": {
    "describe": true,
    "xdescribe": true,
    "before": true,
